<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context" xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd"> <!-- bean definitions here -->

    <bean id="Student" class="org.example.Student">
        <constructor-arg index="0" value="1"/>
        <constructor-arg index="1" value="testName"/>
        <constructor-arg index="2">
            <list>
                <ref bean="Phone"/>
                <ref bean="Phone"/>
            </list>
        </constructor-arg>
        <constructor-arg index="3" ref="Address"/>
    </bean>

    <bean id="Address" class="org.example.Address">
        <property name="city" value="Chicago"/>
        <property name="state" value="Illinois"/>
        <property name="country" value="United States"/>
        <property name="zipcode" value="47096"/>
    </bean>

    <bean id="Phone" class="org.example.Phone">
        <constructor-arg index="0" value="testPhone"/>
    </bean>
</beans>